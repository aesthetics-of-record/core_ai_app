generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Image {
  id        Int      @id @default(autoincrement())
  imageUrl  String
  prompt    String
  createdAt DateTime @default(now())
}
